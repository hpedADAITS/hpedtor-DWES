{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
