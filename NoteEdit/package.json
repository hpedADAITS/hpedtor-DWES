{
  "name": "noteedit",
  "version": "1.0.0",
  "description": "gestor sencillo de notas en node",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "notas": "node index.cjs",
    "notas:crear": "node index.cjs --create",
    "notas:editar": "node index.cjs --edit",
    "notas:eliminar": "node index.cjs --delete",
    "notas:leer": "node index.cjs --read",
    "notas:migrar:mongo": "node scripts/migrar-notas-mongo.cjs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "mongoose": "^9.0.1",
    "morgan": "^1.10.1",
    "winston": "^3.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "sonarqube-scanner": "^3.1.0",
    "supertest": "^7.1.1"
  }
}
